#lang racket
(define (filtered-accumulate combiner null-value term a next b predicate)
  (cond ((> a b)
         null-value)
        ((predicate a)
         (combiner (term a)
                   (filtered-accumulate combiner null-value term (next a) next b)))
        (else (filtered-accumulate combiner null-value term (next a) next b))))
  